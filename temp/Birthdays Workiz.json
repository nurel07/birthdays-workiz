    {
      "parameters": {
        "jsCode": "const RAILWAY_BASE_URL = \"https://radiant-illumination-production-911f.up.railway.app\";\n\nreturn $input.all().map(item => {\n  const data = item.json;\n  \n  // Extract employee ID from Personio URL\n  // Format: https://api.personio.de/v1/company/employees/{id}/profile-picture\n  const match = data.imageUrl.match(/employees\\/(\\d+)\\/profile-picture/);\n  const employeeId = match ? match[1] : null;\n  \n  return {\n    json: {\n      name: data.name,\n      imageUrl: employeeId \n        ? `${RAILWAY_BASE_URL}/api/image/${employeeId}` \n        : data.imageUrl,\n      birthdayString: data.birthdayString\n    }\n  };\n});"
      },
