              "id": "24b3f969-9e90-4454-8fe6-5fd35b8a4651",
              "leftValue": "={{\n(() => {\n  // Path based on your current output\n  const bdayStr = $json.attributes.dynamic_1449280?.value; \n  if (!bdayStr) return false;\n\n  const bdayDate = new Date(bdayStr);\n  const today = new Date(); // It is Jan 19, 2026\n\n  // 1. Force the birthday to the current year (2026)\n  const bdayThisYear = new Date(today.getFullYear(), bdayDate.getMonth(), bdayDate.getDate());\n\n  // 2. Define the \"Week Window\" (Sunday to Saturday)\n  const startOfWeek = new Date(today);\n  startOfWeek.setDate(today.getDate() - today.getDay());\n  startOfWeek.setHours(0, 0, 0, 0);\n\n  const endOfWeek = new Date(startOfWeek);\n  endOfWeek.setDate(startOfWeek.getDate() + 6);\n  endOfWeek.setHours(23, 59, 59, 999);\n\n  // 3. Return true if birthday falls in this window\n  return bdayThisYear >= startOfWeek && bdayThisYear <= endOfWeek;\n})()\n}}",
